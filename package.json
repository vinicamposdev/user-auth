{
  "name": "api-template",
  "version": "1.0.0",
  "description": "Serverless template using Typescript",
  "repository": {
    "url": "git+https://github.com/vinicamposdev/user-auth.git"
  },
  "scripts": {
    "test": "jest --runInBand --no-cache",
    "test:unit": "jest --passWithNoTests --runInBand --no-cache --watch -c jest-unit-config.js",
    "sonar": "node sonar-project.js",
    "start": "nodemon --module",
